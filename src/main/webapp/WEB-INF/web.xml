<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>affichervacance</servlet-name>
        <servlet-class>org.projet.servlets.affichervacance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProgrammerReunion</servlet-name>
        <servlet-class>org.projet.servlets.ProgrammerReunion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>org.projet.servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>org.projet.servlets.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjoutterVacance</servlet-name>
        <servlet-class>org.projet.servlets.AjoutterVacance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValiderTache</servlet-name>
        <servlet-class>org.projet.servlets.ValiderTache</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>affichervacance</servlet-name>
        <url-pattern>/affichervacance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProgrammerReunion</servlet-name>
        <url-pattern>/ProgrammerReunion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjoutterVacance</servlet-name>
        <url-pattern>/AjoutterVacance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValiderTache</servlet-name>
        <url-pattern>/ValiderTache</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
